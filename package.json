{
  "name": "tela-admin",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --fix . || exit 0",
    "start": "http-server ./src",
    "clean": "shx rm -rf dist installer",
    "build": "npm run clean && gulp",
    "compile": "node installer.js",
    "pack": "npm run build && npm run pack-admin && npm run pack-driller",
    "pack-driller": "electron-packager dist/driller Driller --platform win32 --arch all --out /.xpd/dist --overwrite --icon dist/driller/assets/icon/logo.ico",
    "pack-admin": "electron-packager dist/admin Admin --platform win32 --arch all --out /.xpd/dist --overwrite --icon dist/admin/assets/icon/logo.ico"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-animate": "1.5.9",
    "angular-aside": "1.3.2",
    "angular-route": "1.5.9",
    "angular-spinner": "1.0.1",
    "angular-toastr": "2.1.1",
    "angular-ui-bootstrap": "2.1.4",
    "bootstrap": "3.3.7",
    "d3": "3.5.17",
    "d3-scale": "1.0.5",
    "highcharts": "5.0.5",
    "highcharts-multicolor-series": "2.2.1",
    "ngstorage": "0.3.11",
    "progress": "^2.0.0",
    "request": "2.74.0",
    "socket.io-client": "1.7.3",
    "tableexport": "^4.0.11",
    "xlsx": "^0.11.18"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.24.1",
    "electron": "1.6.11",
    "electron-packager": "8.7.2",
    "electron-winstaller": "2.6.3",
    "eslint": "^4.12.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-concat": "2.6.1",
    "gulp-html-replace": "1.6.2",
    "http-server": "^0.11.1",
    "shx": "^0.2.2"
  },
